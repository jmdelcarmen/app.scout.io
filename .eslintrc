{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-unused-vars": [1, {"vars": "local", "args": "none"}],
    "no-param-reassign": 0,
    "no-use-before-define": ["error", { "functions": false, "classes": true }],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "max-len": ["error", { "code": 150 }]
  }
}
